<div class="col-xl-4 col-lg-6 col-md-6">
  <div class="card" data-id="<%= project.id %>">
    <%= image_tag project.thumb_image unless project.thumb_image.nil? %>
    <p class="card-text">
      <span><%= link_to project.title, project_path(project) %></span> <%= project.subtitle %>
    </p>
  </div>
</div>
