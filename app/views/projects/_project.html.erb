<div class="col-xl-4 col-lg-6 col-md-6">
  <div class="card" data-id="<%= project.id %>">
    <%= link_to image_tag(project.thumb_image), project_path(project) %>
    <h4 class="card-title"><%= link_to project.title, project_path(project) %></h4>
    <p class="card-text">
      <%= project.subtitle %>
    </p>
  </div>
</div>
